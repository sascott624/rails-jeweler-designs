<h1>Design Index</h1>

<ul><% @designs.each do |design| %>
  <li><%= link_to design.model, design_path(design) %> - <%= design.metal %> - <%= design.stone.name %></li>
  <% end %>
</ul>

<% if current_user %>
  <%= link_to "Add a New Design", new_user_design_path(current_user, @design) %>
<% end %>
